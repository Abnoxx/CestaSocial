<section>
    <div>
        <ul>
            <p id="frase-solic">Solicitações de Cesta Básica</p>
            <% solicitacoes.forEach(solicitacao => { %>
                <div id="solicitacoes">
                    <div class="item">
                        <h2>Nome:</h2>
                        <p>
                            <%=solicitacao.nome%>
                        </p>
                    </div>
                    <div class="item">
                        <h2>CPF:</h2>
                        <p>
                            <%=solicitacao.cpf%>
                        </p>
                    </div>
                    <div class="item">
                        <h2>Telefone:</h2>
                        <p>
                            <%=solicitacao.telefone%>
                        </p>
                    </div>
                    <div class="item">
                        <h2>Endereço:</h2>
                        <p>
                            <%=solicitacao.endereco%>
                        </p>
                    </div>
                    <div class="item">
                        <h2>Número de dependentes:</h2>
                        <p>
                            <%=solicitacao.numeroDependentes%>
                        </p>
                    </div>
                    <div class="item">
                        <h2>Renda mensal:</h2>
                        <p>
                            <%=solicitacao.rendaMensal%>
                        </p>
                    </div>
                    <div class="item">
                        <h2>Data:</h2>
                        <p id="criado-em">
                            <%=solicitacao.createdAt%>
                        </p>
                    </div>
                    <div class="div-button">
                        <a class="btn-slice" href="/concluir/<%=solicitacao.id%>">
                            <button type="button" class="button">Concluída</button>
                        </a>
                    </div>
                </div>
                <% }); %>
    </div>

    </ul>
    </div>
</section>