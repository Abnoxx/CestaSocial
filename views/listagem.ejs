<section>
    <div class="container-fluid">
        <div class="row">
            <% solicitacoes.forEach(solicitacao => { %>
                <div class="col-lg-4" id="solicitacoes">
                    <div class="item">
                        <h2>Nome:</h2>
                        <p>
                            <%=solicitacao.nome%>
                        </p>
                    </div>
                    <div class="item">
                        <h2>Telefone:</h2>
                        <p>
                            <%=solicitacao.telefone%>
                        </p>
                    </div>
                    
                    <div class="item">
                        <h2>Endere√ßo:</h2>
                        <p>
                            <%=solicitacao.endereco%>
                        </p>
                    </div>

                    <div class="div-button">
                        <a class="btn-slice" href="/admin/solicitacoes/infos/<%=solicitacao.id%>">
                            <button type="button" class="button">Ver mais</button>
                        </a>
                    </div>
                </div>
            <% }); %>
        </div>
    </div>
</section>