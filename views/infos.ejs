<section>
    <div class="container">
        <div class="row">
            <div class="col-lg">
                <% solicitacoes.forEach(solicitacao => { %>
                    <div class="col-lg-4" id="solicitacoes">
                        <div class="item">
                            <h2>Nome:</h2>
                            <p>
                                <%=solicitacao.nome%>
                            </p>
                        </div>
                        <div class="item">
                            <h2>Telefone:</h2>
                            <p>
                                <%=solicitacao.telefone%>
                            </p>
                        </div>
                        <div class="item">
                            <h2>Endereço:</h2>
                            <p>
                                <%=solicitacao.endereco%>
                            </p>
                        </div>
                        <div class="item">
                            <h2>Nome:</h2>
                            <p>
                                <%=solicitacao.cpf%>
                            </p>
                        </div>
                        <div class="item">
                            <h2>Nome:</h2>
                            <p>
                                <%=solicitacao.telefone%>
                            </p>
                        </div>
                        <div class="item">
                            <h2>Nome:</h2>
                            <p>
                                <%=solicitacao.numeroDependentes%>
                            </p>
                        </div>
                        <div class="item">
                            <h2>Nome:</h2>
                            <p>
                                <%=solicitacao.rendaMensal%>
                            </p>
                        </div>
                        <div class="item">
                            <h2>Data:</h2>
                            <p id="criado-em">
                                <%=solicitacao.createdAt%>
                            </p>
                        </div>
                        <div class="div-button">
                            <a class="btn-slice" href="statusAtendido">
                                <button type="button" class="button">Atendido</button>
                            </a>
                        </div>
                        <div class="div-button">
                            <a class="btn-slice" href="statusNaoAtendido">
                                <button type="button" class="button">Não atendido</button>
                            </a>
                        </div>
                    </div>
                <% }); %>
            </div>
        </div>
    </div>
</section>